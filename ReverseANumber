import java.util.*;
import java.io.*;

public class Main {
  public static void main(String args[]) throws IOException {
    
    //write your code here
    Scanner sc = new Scanner(System.in);
    int N = sc.nextInt();
    int rev =0;
    while(N!=0){
      int digit = N%10;
      N = N/10;
      rev = rev*10 + digit;
    }
    System.out.println(rev);
    
  }
}
